    <articleMain id="2098" key="c174b606-1537-4388-9ab8-96f4f7aa8178" parentID="1086" level="2" creatorID="0" sortOrder="6" createDate="2018-09-19T11:06:32" updateDate="2018-11-07T09:43:33" nodeName="Articles" urlName="articles" path="-1,1086,2098" isDoc="" nodeType="1084" creatorName="robert" writerName="robert" writerID="0" template="1083" nodeTypeAlias="articleMain" isPublished="true">
